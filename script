local CurrentTradeData = {

    Trading = false,
    User = nil,
    RoflipId = nil,
    Items = {},
    ToRemove = {},
    StartTick = nil

}

local function ChatSay(Message)

    ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents").SayMessageRequest:FireServer(Message,"normalchat")

end

ChatSay("BRUH")

ReplicatedStorage.Trade.UpdateTrade.OnClientEvent:Connect(function(Trade)
    ChatSay("Skibidi")
)
